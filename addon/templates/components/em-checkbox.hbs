{{#em-form-group model=model property=property label=label form=form
  label=label yieldInLabel=yieldInLabel
  labelWrapperClass=labelWrapperClass labelClass=labelClass
  help=help controlWrapper=controlWrapper
  inputId=inputId validationIcons=validationIcons canShowErrors=canShowErrors inputComponent=this}}
  {{input
    id=inputId
    type='checkbox'
    checked=model.userAgree
    disabled=disabled
    required=required
    autofocus=autofocus
    readonly=readonly
    title=title
    class=(concat "form-control " elementClass)
    name=name}}
    {{checked}}
    {{property}}
    {{model.userAgree}}
{{/em-form-group}}
